{
  "name": "prerenderwebnodeplugin",
  "version": "1.0.171",
  "license": "CC-BY-3.0",
  "description": "Pre-rendering hook for webNode applications.",
  "homepage": "https://torben.website/preRenderWebNodePlugin",
  "documentationWebsite": {
    "name": "preRenderWebNodePlugin",
    "trackingCode": "UA-40192634-28"
  },
  "keywords": [
    "web",
    "backend",
    "static",
    "prerender",
    "prerendering",
    "pre-render",
    "pre-rendering"
  ],
  "files": [
    "index.ts",
    "type.ts"
  ],
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/thaibault/preRenderWebNodePlugin.git"
  },
  "scripts": {
    "build": "webOptimizer build",
    "check": "yarn check:types; yarn lint",
    "check:types": "webOptimizer check:types",
    "clear": "webOptimizer clear",
    "document": "webOptimizer document",
    "lint": "webOptimizer lint",
    "prepare": "yarn build",
    "report:coverage": "nyc report --reporter=text-lcov | coveralls",
    "serve": "yarn build && webNode",
    "start": "yarn serve",
    "test": "webOptimizer test",
    "update": "yarn version --patch && yarn publish",
    "update:documentation": "documentationWebsite --log-level info",
    "watch": "webOptimizer build --watch"
  },
  "devDependencies": {
    "@types/jest": "*",
    "@types/loader-utils": "*",
    "@types/node": "*",
    "@types/rimraf": "*",
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "babel-eslint": "*",
    "clientnode": "*",
    "documentation-website": "*",
    "eslint": "*",
    "eslint-config-google": "*",
    "eslint-plugin-jsdoc": "*",
    "jest": "*",
    "jsdoc": "*",
    "web-node": "*",
    "weboptimizer": "*",
    "webpack": "*",
    "webpack-cli": "*",
    "webpack-dev-server": "*"
  },
  "peerDependencies": {
    "@babel/runtime": "*",
    "clientnode": "*",
    "web-node": "*"
  },
  "sideEffects": true,
  "webNode": {
    "preRender": {
      "cache": false,
      "locations": {
        "executer": {
          "exclude": [
            "backup",
            "database",
            "log",
            "mockup",
            "node_modules",
            "src",
            "source"
          ],
          "fileNames": "preRender.js",
          "include": []
        },
        "output": {
          "directoryNames": "preRendered",
          "exclude": []
        }
      },
      "renderAfterConfigurationUpdates": false
    }
  },
  "webOptimizer": {
    "targetTechnology": "node"
  }
}
