{
  "name": "prerenderwebnodeplugin",
  "version": "1.0.12",
  "license": "CC-BY-3.0",
  "description": "Pre-rendering hook for webNode applications.",
  "homepage": "http://torben.website/preRenderWebNodePlugin",
  "documentationWebsite": {
    "name": "preRenderWebNodePlugin",
    "trackingCode": "UA-40192634-25"
  },
  "keywords": [
    "web",
    "backend",
    "static",
    "prerender",
    "prerendering",
    "pre-render",
    "pre-rendering"
  ],
  "files": [
    "index.js"
  ],
  "main": "index.compiled.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/thaibault/preRenderWebNodePlugin.git"
  },
  "scripts": {
    "build": "webOptimizer build",
    "build:dll": "webOptimizer build:dll",
    "clear": "webOptimizer clear",
    "document": "webOptimizer document",
    "lint": "webOptimizer check:type; webOptimizer lint",
    "prepare": "npm run build",
    "serve": "npm run build && webNode",
    "start": "npm run serve",
    "test": "webOptimizer test",
    "update": "npm version patch && npm publish",
    "update:documentation": "documentationWebsite --log-level info",
    "watch": "webOptimizer build --watch",
    "watch:dll": "webOptimizer build:dll --watch"
  },
  "dependencies": {
    "clientnode": "^1.0.247"
  },
  "peerDependencies": {
    "web-node": "^1.0.116"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "documentation-website": "^1.0.66",
    "eslint": "^4.8.0",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-flowtype": "^2.36.0",
    "eslint-plugin-jsdoc": "^3.1.1",
    "flow-bin": "^0.57.2",
    "jsdoc": "^3.5.2",
    "source-map-support": "^0.5.0",
    "web-node": "^1.0.119",
    "weboptimizer": "^1.0.377",
    "webpack": "^3.7.1",
    "webpack-dev-server": "^2.9.1"
  },
  "webNode": {
    "preRender": {
      "cache": false,
      "locationsToIgnore": [
        "backup",
        "database",
        "log",
        "mockup",
        "node_modules"
      ],
      "fileBaseNames": [
        "preRender"
      ],
      "renderAfterConfigurationUpdates": false
    }
  },
  "webOptimizer": {
    "module": {
      "aliases": {
        "clientnode/test$": "clientnode/test.compiled",
        "web-node/configurator$": "web-node/configurator.compiled",
        "web-node/pluginAPI$": "web-node/pluginAPI.compiled"
      },
      "optimizer": {
        "babelMinify": null
      },
      "preprocessor": {
        "javaScript": {
          "options": {
            "presets": {
              "__evaluate__": "(self.targetTechnology === 'node' ? ['es2017-node7'] : [['modern-browsers', {loose: true}], 'es2017']).concat('stage-0')"
            }
          }
        }
      }
    }
  }
}
