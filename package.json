{
  "name": "prerenderwebnodeplugin",
  "version": "1.0.72",
  "license": "CC-BY-3.0",
  "description": "Pre-rendering hook for webNode applications.",
  "homepage": "http://torben.website/preRenderWebNodePlugin",
  "documentationWebsite": {
    "name": "preRenderWebNodePlugin",
    "trackingCode": "UA-40192634-28"
  },
  "keywords": [
    "web",
    "backend",
    "static",
    "prerender",
    "prerendering",
    "pre-render",
    "pre-rendering"
  ],
  "files": [
    "index.js"
  ],
  "main": "index.compiled.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/thaibault/preRenderWebNodePlugin.git"
  },
  "scripts": {
    "build": "webOptimizer build",
    "clear": "webOptimizer clear",
    "document": "webOptimizer document",
    "lint": "webOptimizer check:type; webOptimizer lint",
    "prepare": "npm run build",
    "report:coverage": "nyc report --reporter=text-lcov | coveralls",
    "serve": "npm run build && webNode",
    "start": "npm run serve",
    "test": "nyc npm run test:plain",
    "test:plain": "webOptimizer test",
    "update": "npm version patch && npm publish",
    "update:documentation": "documentationWebsite --log-level info",
    "watch": "webOptimizer build --watch"
  },
  "dependencies": {
    "clientnode": "^3.0.72"
  },
  "peerDependencies": {
    "babel-runtime": "*",
    "web-node": "*"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "coveralls": "^3.0.2",
    "documentation-website": "^1.0.134",
    "eslint": "^5.3.0",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-jsdoc": "^3.7.1",
    "flow-bin": "^0.78.0",
    "jsdoc": "^3.5.2",
    "nyc": "^11.9.0",
    "qunit": "^2.6.1",
    "web-node": "^1.0.189",
    "weboptimizer": "^2.0.177",
    "webpack": "^4.16.5",
    "webpack-cli": "^2.1.5",
    "webpack-dev-server": "^3.1.5"
  },
  "webNode": {
    "preRender": {
      "cache": false,
      "locationsToIgnore": [
        "backup",
        "database",
        "log",
        "mockup",
        "node_modules"
      ],
      "fileBaseNames": [
        "preRender"
      ],
      "renderAfterConfigurationUpdates": false
    }
  },
  "webOptimizer": {
    "files": {
      "compose": {
        "cascadingStyleSheet": null
      }
    },
    "module": {
      "aliases": {
        "clientnode/test$": "clientnode/test.compiled",
        "web-node/configurator$": "web-node/configurator.compiled",
        "web-node/pluginAPI$": "web-node/pluginAPI.compiled"
      },
      "optimizer": {
        "babelMinify": {
          "module": {
            "mangle": false
          }
        }
      }
    },
    "targetTechnology": "node"
  }
}
