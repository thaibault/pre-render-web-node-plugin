{
  "name": "prerenderwebnodeplugin",
  "version": "1.0.187",
  "license": "CC-BY-3.0",
  "description": "Pre-rendering hook for web-node applications.",
  "homepage": "https://torben.website/pre-render-web-node-plugin",
  "documentationWebsite": {
    "name": "pre-render-web-node-plugin",
    "trackingCode": "UA-40192634-28"
  },
  "keywords": [
    "web",
    "backend",
    "static",
    "prerender",
    "prerendering",
    "pre-render",
    "pre-rendering"
  ],
  "files": [
    "index.ts",
    "type.ts"
  ],
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/thaibault/pre-render-web-node-plugin.git"
  },
  "scripts": {
    "build": "weboptimizer build",
    "check": "yarn check:types; yarn lint",
    "check:types": "weboptimizer check:types",
    "clear": "weboptimizer clear",
    "document": "weboptimizer document",
    "lint": "weboptimizer lint",
    "prepare": "yarn build",
    "report:coverage": "nyc report --reporter=text-lcov | coveralls",
    "serve": "yarn build && webNode",
    "start": "yarn serve",
    "test": "weboptimizer test",
    "update": "yarn version --patch && yarn publish",
    "update:documentation": "documentationWebsite --log-level info",
    "watch": "weboptimizer build --watch"
  },
  "devDependencies": {
    "@types/jest": "*",
    "@types/loader-utils": "*",
    "@types/node": "*",
    "@types/rimraf": "*",
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "babel-eslint": "*",
    "clientnode": "*",
    "documentation-website": "*",
    "eslint": "*",
    "eslint-config-google": "*",
    "eslint-plugin-jsdoc": "*",
    "jest": "*",
    "jsdoc": "*",
    "web-node": "*",
    "weboptimizer": "*"
  },
  "peerDependencies": {
    "@babel/runtime": "*",
    "clientnode": "*",
    "web-node": "*"
  },
  "sideEffects": true,
  "webNode": {
    "preRender": {
      "cache": false,
      "locations": {
        "executer": {
          "exclude": [
            "backup",
            "database",
            "log",
            "mockup",
            "node_modules",
            "src",
            "source"
          ],
          "fileNames": "preRender.js",
          "include": []
        },
        "output": {
          "directoryNames": "preRendered",
          "exclude": []
        }
      },
      "renderAfterConfigurationUpdates": false
    }
  },
  "webOptimizer": {
    "targetTechnology": {
      "boilerplate": "node",
      "payload": "node"
    }
  }
}
